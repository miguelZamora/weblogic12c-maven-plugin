  <display-name>SayHelloWebApp</display-name>
  <servlet>
    <servlet-name>SayHelloServlethttp</servlet-name>
    <servlet-class>cl.cachoza.app001ws.SayHello</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>SayHelloServlethttp</servlet-name>
    <url-pattern>/SayHello</url-pattern>
  </servlet-mapping>
</web-app>
